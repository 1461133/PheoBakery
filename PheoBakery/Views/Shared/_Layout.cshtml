<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <title>@ViewBag.Title DapueKue - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    

</head>
<body>
    <div id="warp">
        <div id="main" class="container_16">
            <div id="header" class="grid_16">
                <div id="logo" class="grid_4 alpha">
                    <h1><a href="@Url.Action("Index","Home")">DapurKue</a></h1>
                    <h2>Famously Delicious</h2>
                </div>
                <div id="headright" class="grid_7 prefix_5 omega">
                    <h3 class="login"><a href="@Url.Action("Index","Home")">Sign up</a> / <a href="@Url.Action("DangNhap","Admin")">Login</a></h3>
                    
                    <div class="col-md-6 top-header-left">
                        @Html.Action("GioHangPartial", "GioHang")
                    </div>
                 </div>
            </div>

        </div>
        <div class="clear"></div>
    </div>
    @{Html.RenderAction("MenuPartial", "Home");}

    @RenderBody()
    
    @Scripts.Render("~/bundles/scripts")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
